{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "yarn"
  },
  "newProjectRoot": "projects",
  "projects": {
    "sanity-app-esm": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/sanity-app-esm",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "esm": {
              "customWebpackConfig": {
                "path": "custom-webpack.config.js"
              },
              "indexTransform": "index.transform.js"
            },
            "cjs": {
              "customWebpackConfig": {
                "path": "custom-webpack.config.cjs"
              },
              "indexTransform": "index.transform.cjs"
            }
          },
          "defaultConfiguration": "production,esm"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "port": 5005
          },
          "configurations": {
            "production": {
              "browserTarget": "sanity-app-esm:build:production"
            },
            "development": {
              "browserTarget": "sanity-app-esm:build:development"
            },
            "cjs": {
              "browserTarget": "sanity-app-esm:build:cjs"
            },
            "esm": {
              "browserTarget": "sanity-app-esm:build:esm"
            }
          },
          "defaultConfiguration": "esm"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sanity-app-esm:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "sanity-app-esm:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sanity-app-esm:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "watch": true,
            "headless": false
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "sanity-app-esm:serve",
            "browser": "chrome",
            "port": 4224
          },
          "configurations": {
            "production": {
              "devServerTarget": "sanity-app-esm:serve:production"
            },
            "esm": {
              "devServerTarget": "sanity-app-esm:serve:esm"
            },
            "cjs": {
              "devServerTarget": "sanity-app-esm:serve:cjs"
            }
          },
          "defaultConfiguration": "esm"
        }
      }
    }
  },
  "defaultProject": "sanity-app-esm"
}
