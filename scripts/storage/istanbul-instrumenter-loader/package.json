		"3.0.0-beta.0": {
			"name": "istanbul-instrumenter-loader",
			"version": "3.0.0-beta.0",
			"description": "Istanbul instrumenter loader for webpack",
			"author": {
				"name": "Kir Belevich",
				"url": "@deepsweet"
			},
			"main": "dist/cjs.js",
			"files": [
